{
  "name": "@phanee.tk/notebook",
  "version": "1.0.0",
  "description": "Personal notebook and documentation website",
  "scripts": {
    "dev": "NODE_ENV=development next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "export": "next build && next export -o dist",
    "test": "jest",
    "test:cov": "yarn test --coverage",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "format": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx --fix src",
    "clean": "rimraf ./{tsconfig.tsbuildinfo,dist,node_modules,yarn-error.log,yarn.lock,.next}",
    "updatedeps": "ncu"
  },
  "keywords": [
    "next.js",
    "react.js",
    "blog",
    "mdx"
  ],
  "author": "phaneendra",
  "license": "ISC",
  "bin": {
    "blog": "src/modules/cli/cli.js"
  },
  "browserslist": [
    "last 2 version"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@mdx-js/loader": "^1.5.1",
    "@next/mdx": "^9.1.4",
    "@zeit/next-css": "^1.0.1",
    "config": "^3.2.4",
    "moment": "^2.24.0",
    "next": "latest",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^0.19.0",
    "next-seo": "^4.4.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "remark-emoji": "^2.0.2",
    "remark-images": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "npm-run-all": "^4.1.5",
    "npm-check-updates": "^6.0.1",
    "rimraf": "^3.0.2",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^24.5.0",
    "next-optimized-images": "^2.5.4",
    "preval.macro": "^4.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.4",
    "svg-sprite-loader": "^4.1.6",
    "webp-loader": "^0.5.0",
    "webpack": "^4.42.0"
  }
}
