{
  "name": "@phanee.tk/notebook",
  "version": "1.0.0",
  "description": "Personal notebook and documentation website",
  "scripts": {
    "dev": "NODE_ENV=development next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "export": "next build && next export -o dist",
    "test": "jest",
    "test:cov": "yarn test --coverage",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "format": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx --fix src",
    "clean": "rimraf ./{tsconfig.tsbuildinfo,dist,node_modules,yarn-error.log,yarn.lock,.next}",
    "updatedeps": "ncu",
    "chakracli": "chakra-cli"
  },
  "keywords": [
    "next.js",
    "react.js",
    "blog",
    "mdx"
  ],
  "author": "phaneendra",
  "license": "ISC",
  "bin": {
    "blog": "src/modules/cli/cli.js"
  },
  "browserslist": [
    "last 2 version"
  ],
  "dependencies": {
    "@chakra-ui/button": "1.0.0-rc.0",
    "@chakra-ui/color-mode": "1.0.0-rc.0",
    "@chakra-ui/css-reset": "1.0.0-rc.0",
    "@chakra-ui/layout": "1.0.0-rc.0",
    "@chakra-ui/system": "1.0.0-rc.0",
    "@chakra-ui/theme": "1.0.0-rc.0",
    "@chakra-ui/theme-tools": "^1.0.0-next.3",
    "@chakra-ui/utils": "1.0.0-next.7",
    "@emotion/core": "^10.0.28",
    "@mdx-js/loader": "^1.6.16",
    "@next/mdx": "^9.5.1",
    "@zeit/next-css": "^1.0.1",
    "config": "^3.3.1",
    "moment": "^2.27.0",
    "next": "latest",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.4.0",
    "next-seo": "^4.7.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "remark-emoji": "^2.1.0",
    "remark-images": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chakra-cli": "^1.0.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jest": "^26.2.1",
    "next-optimized-images": "^2.6.2",
    "npm-check-updates": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "preval.macro": "^5.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "svg-sprite-loader": "^5.0.0",
    "typescript": "^3.9.7",
    "webp-loader": "^0.6.0",
    "webpack": "^4.44.1"
  }
}
